<div class="container">
  <div class="container-fluid">

    <% @articles.each do |a| %>
        <div class="jumbotron">

          <h1>
            <%= a.title %>
          </h1>

          <p class="text-center">
            <%= link_to "Lire l'article", article_path(a) %>
          </p>

        </div>
    <% end %>
  </div>
</div>